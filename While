using System;

namespace While
{
    class MainApp
    {
        static void Main(string[] args)
        {
            int i = 10;

            while(i>0)
            {
                Console.WriteLine($"i : {i--}");
            }
         
        }
    }
}

// while문
// while(조건식)
// {
//    반복실행할_코드_블록
// }
